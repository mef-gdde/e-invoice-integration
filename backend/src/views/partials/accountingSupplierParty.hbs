<cac:AccountingSupplierParty>
  <cac:Party>
    {{! Khmer Vatin Number}}
    {{!-- <cbc:EndpointID {{!schemeID="0088" }}>{{supplier.endpoint_id}}</cbc:EndpointID> --}}
    {{#if supplier.id}}
    <cac:PartyIdentification>
      <cbc:ID>{{supplier.id}}</cbc:ID>
    </cac:PartyIdentification>
    {{/if}}
    {{#if supplier.party_legal_entity.registration_name_kh}}
    <cac:PartyName>
      <cbc:Name>{{supplier.party_legal_entity.registration_name_kh}}</cbc:Name>
    </cac:PartyName>
    {{/if}}
    <cac:PostalAddress>
      {{!-- <cbc:StreetName>{{supplier.postal_address.street_name}}</cbc:StreetName> --}}
      <cbc:CityName>{{supplier.postal_address.city_name}}</cbc:CityName>
      <cac:Country>
        <cbc:IdentificationCode>{{supplier.postal_address.country_code}}</cbc:IdentificationCode>
      </cac:Country>
    </cac:PostalAddress>
    <cac:PartyTaxScheme>
      {{!Vatin Number}}
      <cbc:CompanyID>{{supplier.party_tax_scheme.company_id}}</cbc:CompanyID>
      <cac:TaxScheme>
        {{!VAT}}
        <cbc:ID>{{supplier.party_tax_scheme.tax_scheme}}</cbc:ID>
      </cac:TaxScheme>
    </cac:PartyTaxScheme>
    <cac:PartyLegalEntity>
      <cbc:RegistrationName>{{supplier.party_legal_entity.registration_name_en}}</cbc:RegistrationName>
      <cbc:CompanyID>{{supplier.party_legal_entity.company_id}}</cbc:CompanyID>
    </cac:PartyLegalEntity>
    {{#if supplier.contact}}
    <cac:Contact>
      <cbc:Telephone>{{supplier.contact.phone}}</cbc:Telephone>
      <cbc:ElectronicMail>{{supplier.contact.email}}</cbc:ElectronicMail>
    </cac:Contact>
    {{/if}}
  </cac:Party>
</cac:AccountingSupplierParty>